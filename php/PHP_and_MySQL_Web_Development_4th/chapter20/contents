  <SCRIPT language = "javascript">
		function openW(mypage,myname,w,h) 
		{
			baseHref = "http://" + location.hostname;

			mypage = baseHref + mypage;
			if(screen.width){
			var winl = (screen.width-w)/2;
			var wint = (screen.height-h)/2;
			}else{winl = 0;wint =0;}
			if (winl < 0) winl = 0;
			if (wint < 0) wint = 0;

			var settings = 'height=' + h + ',';
			settings += 'width=' + w + ',';
			settings += 'top=' + wint + ',';
			settings += 'left=' + winl + ',';
			
			win = window.open(mypage,myname,settings);
			
			win.window.focus();
		}

		function setProductName(symbol)
		{
			top.productName = symbol;
			
		}
  </SCRIPT>	  
